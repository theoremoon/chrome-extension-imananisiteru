{
    "name": "imananisiteru",
    "description": "Ask you imananisiteru",
    "version": "1.0.0",
    "manifest_version": 2,
    "permissions": [
        "alarms",
        "notifications",
        "tabs",
        "storage"
    ],
    "background": {
        "persistent": false,
        "scripts": ["background.js"]
    },
    "icons": {
        "16": "jikan_tobu.png"
    },
    "browser_action": {
        "default_popup": "popup.html"
    }
}
